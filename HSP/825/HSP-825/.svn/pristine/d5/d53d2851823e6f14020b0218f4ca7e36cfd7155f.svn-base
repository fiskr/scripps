/* HGTV OVERRIDES */
body#design-star {
    background:#E1E6E5;
    }

#design-star .center-mod .hd {
    background:url(../imgs/sp-designstar-promo.gif) no-repeat scroll 0 0;
    width:407px;
    height:3px;
    }

#design-star .center-mod .ft {
    background:url(../imgs/sp-designstar-promo.gif) no-repeat scroll 0 -3px;
    width:407px;
    height:3px;
    }

#design-star .center-mod .bd {
    background-color:#dbecff;
    padding:10px;
    }

#design-star .dstar .flt-rt {
    _width:150px;
    _float:right;
    }

#design-star .dstar p {
    _float:left;
    *width:200px;
    }

#design-star #sponsors {
    text-align:center;
    border-bottom:1px solid #DAE7E3;
    margin:14px;
    padding-bottom:14px;
    }

#design-star #sponsors img {
    border:none;
    padding:0;
    margin:0;
    }

#design-star #sponsors ul {
    margin:0 auto;
    width:430px;
    }

#design-star #sponsors ul li {
    float:left;
    margin-right:10px;
    }

#design-star #sponsors ul li.byline {
    font:normal 77% verdana, arial;
    color:#ccc;
    padding-top:10px;
    }

#design-star #meet-the-designers {
    margin:14px 0;
    }

#design-star #meet-the-designers h2 {
    margin-left:14px;
    }

/*#design-star #meet-the-designers img {margin: 0 auto;}*/
#design-star .view-slider {
    margin-left:14px;
    overflow:auto;
    position:relative;
    width:603px;
    height:190px;
    *height:198px;
    }

#design-star .view-slider ul {
    width:2750px;
    padding:0;
    list-style-type:none;
    }

#design-star .view-slider ul li {
    width:90px;
    height:160px;
    float:left;
    margin-right:0;
    padding:0 4px;
    text-align:left;
    }

#design-star .view-slider ul li img {
    float:left;
    margin:20px 14px 0 0;
    _margin:20px 7px 0 0;
    display:block;
    cursor:pointer;
    border:0;
    width:90px;
    height:120px;
    padding:0;
    background:url(http://images.foodnetwork.com/webfood/fn20/imgs/fn-loader.gif) no-repeat center;
    }

#design-star .view-slider ul li.active {
    position:relative;
    padding:0;
    margin:0 -11px;
    width:303px;
    border:1px solid #0f40a1;
    z-index:500;
    background:url(http://web.hgtv.com/webhgtv/hg20/pkgs/2011/design-star/images/carousel-bg.jpg) no-repeat scroll 0 0;
    }

#design-star .view-slider ul li.first, #design-star .view-slider ul li.last {
    margin:0;
    }

#design-star .view-slider ul li.first.active {
    margin:0 -11px;
    }

#design-star .view-slider ul li.last.active {
    margin:0 -22px 0 -11px;
    }

#design-star .view-slider ul li.active img {
    width:120px;
    height:160px;
    margin-top:0;
    }

#design-star .view-slider ul ul, #design-star .view-slider h4 {
    float:left;
    }

#design-star .view-slider ul ul {
    width:auto;
    margin:0;
    }

#design-star .view-slider h4 {
    font-size:12px;
    margin:4px 0;
    }

#design-star .view-slider ul li div, #design-star .view-slider ul li h3, #design-star .view-slider ul li p {
    display:none;
    }

#design-star .view-slider ul li li {
    padding-left:9px;
    background:url(http://web.hgtv.com/webhgtv/hg20/pkgs/2011/design-star/images/bullet-blue.png) no-repeat left 6px;
    margin-bottom:3px;
    width:156px;
    }

#design-star .view-slider ul li li a {
    height:auto;
    color:#daeaff;
    line-height:1.38em;
    text-decoration:none;
    }

#design-star .view-slider ul li.active li a {
    width:153px;
    }

#design-star .view-slider ul li.active li a:hover {
    text-decoration:underline;
    color:#FFF;
    }

#design-star .view-slider ul li.active div {
    margin-top:50px;
    display:block;
    }

#design-star .view-slider ul li.active a {
    float:left;
    width:130px;
    display:block;
    }

#design-star .view-slider ul li div {
    float:left;
    width:168px;
    }

#design-star .view-slider ul li div li {
    height:auto;
    }

#design-star .view-slider ul li.active h3 {
    position:absolute;
    top:0;
    right:0;
    display:block;
    width:169px;
    height:25px;
    margin:0;
    color:#fff;
    font:bold 123.1% Trebuchet MS;
    padding:6px 0 0 14px;
    }

#design-star .view-slider ul li.active h3 a {
    color:#FFF;
    }

#design-star .view-slider ul li.active h3 a:hover {
    text-decoration:none;
    }

#design-star .view-slider ul li.active p {
    position:absolute;
    top:25px;
    right:0;
    display:block;
    width:169px;
    height:25px;
    margin:0;
    color:#fff;
    font-size:85%;
    padding:2px 0 0 14px;
    }


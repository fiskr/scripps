/* HOT THIS SEASON */
#hot-this-season li {
    border-bottom:1px dashed #c1c1c1;
    padding-bottom:3px;
    margin-bottom:3px;
    }

#hot-this-season li.last {
    border-bottom:none;
    }

#design-star.judge-designs #fullwidth-wrapper, #design-star.casting-call #fullwidth-wrapper {
    background:url("http://web.hgtv.com/webhgtv/hg20/imgs/ds_fullwidth_bcap.png") center bottom no-repeat;
    }

#design-star #pgallery3 {
    margin-left:0;
    margin-right:0;
    }

.pod#star-past .bd {
    padding-right:7px;
    }

	

/*EPISODE PAGE OVERIDES*/
#episode-hdr h2 {
	font-size: 24px;
	color: #444;
	font-weight: normal;
	margin: 0 0 14px 0;
	}

#episode-hdr h3 {
	font-size: 13px;
	}

#episode-hdr ul.episode-nav {
	width: auto;
	margin-top: 6px;
	}

#episode-hdr ul.episode-nav li a {
	color: #386CB4;
	width: auto;
	margin-right: 0;
	}

#episode-hdr img, #episode-videos img, #episode-photos img {
	background: transparent;
	}

#episode-hdr, #episode-videos, #episode-photos, #episode-designers {
	padding-right: 16px;
	}

#episode-photos {
	border-bottom: none;
	}
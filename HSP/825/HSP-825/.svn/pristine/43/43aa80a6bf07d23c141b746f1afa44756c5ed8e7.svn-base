/**
 * This <div> element is wrapped by jCarousel around the list
 * and has the classname "jcarousel-container". */
/* these are the default styling setup for all variations of the carousel
 do not edit until the 'skins' section */
.jcarousel-container {
    position:relative;
    }

.jcarousel-container p {
    margin:0;
    }

.jcarousel-clip {
    z-index:2;
    padding:0;
    margin:0;
    overflow:hidden;
    position:relative;
    }

.jcarousel-list {
    z-index:1;
    overflow:hidden;
    position:relative;
    top:0;
    left:0;
    margin:0;
    padding:0;
    }

.jcarousel-item {
    float:left;
    list-style:none;
    background:none;
    }

.jcarousel-next {
    z-index:3;
    display:none;
    }

.jcarousel-prev {
    z-index:3;
    display:none;
    }

/*.skin-crsl-1c */
.skin-crsl-1c .jcarousel-clip-horizontal, .skin-crsl-1d .jcarousel-clip-horizontal {
    width:166px;
    padding:32px 0 0 0;
    margin:0 10px;
    }

.skin-crsl-1c .jcarousel-item, .skin-crsl-1d .jcarousel-item {
    width:166px;
    text-align:center;
    padding:0px;
    }

.skin-crsl-1c .jcarousel-item p, .skin-crsl-1d .jcarousel-item p {
    text-align:left;
    }

/* page info */
.skin-crsl-1c .jcarousel-pagetext, .skin-crsl-1d .jcarousel-pagetext {
    position:absolute;
    top:14px;
    width:100%;
    display:block;
    text-align:center;
    color:#999;
    font-size:77%; /* 10px */
    line-height:18px; /* vertical align with arrows */;
    }

/* Horizontal Buttons */
.skin-crsl-1c .jcarousel-next-horizontal, .skin-crsl-1d .jcarousel-next-horizontal {
    position:absolute;
    top:68px;
    right:10px;
    width:18px;
    height:18px;
    cursor:pointer;
    background:transparent url(http://phpdev.scrippsnetworks.com/hgtv-archive.09.30.2008/images/buttons.gif) no-repeat -26px 0;
    }

.skin-crsl-1c .jcarousel-next-horizontal:hover, .skin-crsl-1d .jcarousel-next-horizontal:hover {
    background-position:-26px -18px;
    }

.skin-crsl-1c .jcarousel-next-disabled-horizontal, .skin-crsl-1c .jcarousel-next-disabled-horizontal:hover, .skin-crsl-1c .jcarousel-next-disabled-horizontal:active, .skin-crsl-1d .jcarousel-next-disabled-horizontal, .skin-crsl-1d .jcarousel-next-disabled-horizontal:hover, .skin-crsl-1d .jcarousel-next-disabled-horizontal:active {
    cursor:default;
    background-position:-26px -36px;
    }

.skin-crsl-1c .jcarousel-prev-horizontal, .skin-crsl-1d .jcarousel-prev-horizontal {
    position:absolute;
    top:68px;
    left:10px;
    _left:0;
    width:18px;
    height:18px;
    cursor:pointer;
    background:transparent url(http://phpdev.scrippsnetworks.com/hgtv-archive.09.30.2008/images/buttons.gif) no-repeat -8px 0;
    }

.skin-crsl-1c .jcarousel-prev-horizontal:hover, .skin-crsl-1d .jcarousel-prev-horizontal:hover {
    background-position:-8px -18px;
    }

.skin-crsl-1c .jcarousel-prev-disabled-horizontal, .skin-crsl-1c .jcarousel-prev-disabled-horizontal:hover, .skin-crsl-1c .jcarousel-prev-disabled-horizontal:active, .skin-crsl-1d .jcarousel-prev-disabled-horizontal, .skin-crsl-1d .jcarousel-prev-disabled-horizontal:hover, .skin-crsl-1d .jcarousel-prev-disabled-horizontal:active {
    cursor:default;
    background-position:-8px -36px;
    }

/*.skin-crsl-1c h4, .skin-crsl-1d h4{color:#D72D82;text-align:left;}*/
.skin-crsl-1c img, .skin-crsl-1d img {
    margin:0 0 14px;
    }

.skin-crsl-1c p, .skin-crsl-1d p {
    text-align:left;
    }

.skin-crsl-1c cite, .skin-crsl-1d cite {
    color:#195A64;
    margin:3px 0 0 0;
    display:block;
    text-align:left;
    }

.skin-crsl-1c cite span, .skin-crsl-1d cite span {
    color:#333;
    margin:0px 5px 0 0;
    }


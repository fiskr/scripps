/* Super Lead  - rebranding CLewis 2010-Jun  */
.super-lead {
    width:994px;
    height:200px;
    margin:7px auto 14px;
    background:#dae7e3;
    position:relative;
    }

.super-lead h1 {
    width:994px;
    height:200px;
    margin:0;
    text-indent:-999em;
    -moz-border-radius:4px;
    -webkit-border-radius:4px;
    border-radius:4px;
    }

.super-lead h1 span {
    display:block;
    height:100%;
    width:100%;
    }

.super-lead .show-info {
    width:623px;
    position:absolute;
    left:7px;
    bottom:7px;
    background-color:#fff;
    -moz-border-radius:2px;
    -webkit-border-radius:2px;
    border-radius:2px;
    }

.super-lead .show-info .bd {
    padding:7px 0 0;
    }

.super-lead .show-info .ft {
    height:6px;
    _font-size:0; /*padding-bottom: 7px; font-size: 1%;*/

/* join the conversation right rail module -- based on code from FN by Matt Ford */
.pod.join-the-conversation .hd,
.pod.join-the-conversation .bd,
.pod.join-the-conversation .ft { margin: 0; padding: 0 12px; }

.pod.join-the-conversation .hd { padding-bottom: 12px; }
.pod.join-the-conversation .bd { padding-bottom: 4px; }

.pod.join-the-conversation .shares,
.pod.join-the-conversation .social-items { float: left; }

.pod.join-the-conversation .shares { padding-top: 3px; margin-right: 7px; padding-right: 10px; border-right: 1px solid #dfdfdf; height: 67px; }
.pod.join-the-conversation .shares > * { float: left; }

.pod.join-the-conversation .social-items.networks { width: 76px; border-right: 1px solid #dfdfdf; padding-right: 7px; margin-right: 10px; }
.pod.join-the-conversation .social-items.blogs { width: 96px; margin-top: 16px; line-height: 1.4em; }

.pod.join-the-conversation .social-items.networks > li {
	text-indent: -5000px;
	overflow: hidden;
	background: url(../imgs/jtc-social-sprite.png) no-repeat;
	float: left;
	width: 32px;
	height: 32px;
	margin: 0 3px;
}

.pod.join-the-conversation .social-items.networks > li a { display: block; height: 100%; }
.pod.join-the-conversation .social-items.networks > li.row-two { margin-top: 6px; }

.pod.join-the-conversation .social-items.networks .twitter { background-position: 0 0; }
.pod.join-the-conversation .social-items.networks .fb { background-position: -32px 0; }
.pod.join-the-conversation .social-items.networks .pinterest { background-position: -64px 0; }
.pod.join-the-conversation .social-items.networks .instagram { background-position: -96px 0; }
	
	
/* .tvschedule: On HGTV, a special pod with its own bg image  */
#hg-e .tvschedule {
    width:349px;
    margin:14px 0;
    background:#fbefb5 url(../imgs/on-tv.jpg) no-repeat -1px 0;
    border-top-right-radius:4px;
    -moz-border-radius-topright:4px;
    -webkit-border-top-right-radius:4px;
    border-bottom-right-radius:4px;
    -moz-border-radius-bottomright:4px;
    -webkit-border-bottom-right-radius:4px;
    }

#hg-e .tvschedule .hd {
    padding-left:14px;
    height:38px;
    *background:url(../imgs/ie/cnr-mask-wh-tr.png) no-repeat right top;
    _background-image:none;
    }

#hg-e .tvschedule .hd h4 {
    text-indent:-999em;
    }

#hg-e .tvschedule .bd {
    padding:0 14px;
    background:transparent;
    }

#hg-e .tvschedule .ft {
    padding-left:10px;
    height:14px;
    *background:url(../imgs/ie/cnr-mask-wh-br.png) no-repeat right bottom;
    _background-image:none;
    }

.tvschedule li.acco-link {
    height:80px;
    }

.tvschedule li.acco-bd {
    height:80px;
    background:#fefcf0;
    }

.tvschedule .bd li.acco-group .ui-state-active div.bg,
.tvschedule .bd li.acco-group .ui-accordion-content div.bg {
    background:#fefcf0;
    }

.tvschedule .bd li.acco-group .ui-accordion-content div.bg.showimg {
    height:110px;
    }

.tvschedule .bd li.acco-group .ui-accordion-content {
    *overflow:hidden;
    }

.tvschedule .bd li {
    margin-bottom:2px;
    background:#fff;
    padding:5px;
    padding-left:10px;
    overflow:hidden;
    width:306px;
    }

.tvschedule .bd li.episodefinder {
    overflow:visible;
    }

.tvschedule .bd li.selected {
    height:138px;
    overflow:auto;
    overflow-x:hidden;
    position:relative;
    }

.tvschedule .bd li div.bg {
    margin:0 0 2px 56px;
    background:#f1f6f2;
    }

.tvschedule .bd li.selected div {
    background:#fefcf0;
    }

.tvschedule .bd h5 {
    font:bold 116% "Trebuchet MS", sans-serif;
    color:#195a64;
    padding:10px 0 10px 56px;
    margin-left:-56px;
    cursor:pointer;
    width:240px;
    }

.tvschedule .bd .dontmiss {
    background:transparent url(../imgs/onhgtv-slots.gif) no-repeat 0 -71px;
    }

.tvschedule .bd .dontmiss a, .tvschedule .bd .dontmiss a:hover {
    text-decoration:none;
    }

.tvschedule .bd .primetime {
    background:transparent url(../imgs/onhgtv-slots.gif) no-repeat 0 -213px;
    }

.tvschedule .bd .onnow {
    background:transparent url(../imgs/onhgtv-slots.gif) no-repeat 5px -353px;
    }

.tvschedule .bd li h5 span {
    display:block;
    margin-left:4px;
    padding-left:15px;
    }

.tvschedule .bd h5 p {
    padding:17px 0 0 78px;
    margin:-17px 0 0 -59px;
    font:normal 85% arial, sans-serif;
    color:#333;
    _zoom:1;
    height:15px;
    overflow:hidden;
    *z-index:1;
    }

#hg-e .tvschedule .bd li div img {
    border:none;
    padding-bottom:14px;
    padding-left:8px;
    padding-right:8px;
    padding-top:7px;
    background-color:inherit;
    }

.tvschedule .bd li span a:hover {
    text-decoration:none;
    }

.tvschedule .bd li .acco-link {
    _zoom:1;
    }

.tvschedule .bd li.selected .acco-link {
    background:none;
    }

.tvschedule .bd li h5 span.link {
    background:transparent url(../imgs/hgtv_ui_207.png) no-repeat -795px -489px;
    }

.tvschedule .bd li h5 span {
    *z-index:2;
    *position:relative;
    }

.tvschedule .bd li .ui-state-active h5 span.link {
    background-position:-795px -508px;
    }

.tvschedule .bd li.selected span.link {
    background:none;
    }

.tvschedule .bd li span a {
    display:block;
    height:19px;
    overflow:hidden;
    }

.tvschedule .bd li.selected span a {
    position:relative;
    }

.tvschedule .bd li.selected span a:hover {
    text-decoration:underline;
    }

.tvschedule .bd li.selected h5 {
    width:227px;
    cursor:default;
    *width:225px;
    }

.tvschedule li.episodefinder {
    height:auto;
    margin-bottom:0;
    position:static;
    }

.tvschedule .bd li.episodefinder div.bg {
    }

.tvschedule .bd li.episodefinder .episode {
    margin:0px 10px 0 -60px;
    padding:9px 0 0 70px;
    background:transparent url(../imgs/onhgtv-slots.gif) no-repeat 0px -420px;
    }

.tvschedule .bd li.episodefinder .episode p {
    margin:0 0 7px 0;
    }

.tvschedule .bd li.episodefinder .episode .text-box {
    width:230px;
    }

.tvschedule .bd li.episodefinder .episode fieldset {
    }

.tvschedule .bd li.episodefinder .episode fieldset .topic, .tvschedule .bd li.episodefinder .episode fieldset p {
    margin:0 0 10px 0;
    width:230px;
    background:none;
    }

.tvschedule .bd li.episodefinder .episode .btn-find {
    background:transparent url(../imgs/dpl-formassets.png) no-repeat -50px -179px;
    display:block;
    position:relative;
    float:left;
    width:46px;
    height:28px;
    margin:0 0 0 95px;
    _margin:0 0 0 55px;
    border:none;
    text-indent:-9999px;
    vertical-align:top;
    cursor:pointer;
    }

.tvschedule .bd li.episodefinder .episode .btn-find:active, .tvschedule .bd li.episodefinder .episode .btn-find:focus {
    background:transparent url(../imgs/dpl-formassets.png) no-repeat -50px -206px;
    border:none;
    outline:none;
    }

.tvschedule .bd li.episodefinder .episode span.connector {
    color:#666;
    line-height:30px;
    display:inline;
    float:left;
    margin:0 10px;
    }

.tvschedule .episodefinder .dd-list {
    width:230px;
    margin-bottom:9px;
    }

.tvschedule .episodefinder .dd-list dd {
    margin-left:-53px;
    }

.tvschedule .episodefinder .dd-list li {
    position:static;
    }

.tvschedule .episodefinder .text-box {
    margin-bottom:9px;
    }

/*.tvschedule .episodefinder .text-box input { color: #989898; }*/
.tvschedule .episodefinder .dates .date-box {
    float:left;
    width:102px;
    }

.tvschedule #find-episode-to-label {
    float:left;
    padding:4px 6px 0 7px;
    }

.tvschedule .bd li.episodefinder .episode fieldset p.find {
    margin:7px 0 4px 0;
    text-align:center;
    }

.tvschedule .episodefinder .button {
    width:42px;
    }


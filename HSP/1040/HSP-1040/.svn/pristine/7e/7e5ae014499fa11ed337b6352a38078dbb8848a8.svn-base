/*  first, legacy pod styles */

/* left rail #hg-ww pods */
#hg-ww .pod {
    width:196px;
    margin:14px 0;
    border-top-right-radius:4px;
    -moz-border-radius-topright:4px;
    -webkit-border-top-right-radius:4px;
    border-bottom-right-radius:4px;
    -moz-border-radius-bottomright:4px;
    -webkit-border-bottom-right-radius:4px;
    background:#f1f6f2;
    position:relative;
    _zoom:1;
    }

#hg-ww #product-ideas.pod {
    *position:static;
    margin-top:0;
    }

#hg-ww .pod .hd {
    min-height:2px;
    *background:#f1f6f2 url(../imgs/ie/cnr-mask-wh-tr.png) no-repeat right top;
    _background-image:none;
    }

#hg-ww .pod .hd h4 {
    margin:0 14px 0 0;
    padding:10px 0 10px 7px;
    line-height:1.1em;
    background:url(../imgs/border-red-orange.png) 0 bottom repeat-x;
    font-size:108%;
    font-family:"Trebuchet MS", sans-serif;
    color:#ef4141;
    _position:relative;
    }

#hg-ww .pod .bd {
    padding:10px 7px 0;
    }

#hg-ww .pod .ft {
    height:8px;
    *background:#f1f6f2 url(../imgs/ie/cnr-mask-wh-br.png) no-repeat right bottom;
    _background-image:none;
    }

#hg-ww #hgtv-hosts {
    *z-index:102;
    }

#hg-ww #hgtv-hosts .ft {
    *z-index:102;
    }

#hg-ww .pod .drops {
    *z-index:103;
    }


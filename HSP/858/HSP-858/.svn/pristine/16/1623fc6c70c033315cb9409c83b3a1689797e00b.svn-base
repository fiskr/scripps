    border-right:none;
    }

#dream-home #pgallery #photonav .prev a.btn {
    background:url(../imgs/gh-nav-arrows.gif) left 0 no-repeat;
    padding-left:21px;
    }

#dream-home #pgallery #photonav .next a.btn {
    background:url(../imgs/gh-nav-arrows.gif) right -219px no-repeat;
    padding-right:21px;
    }

#dream-home #pgallery #photonav .prev a.btn:hover {
    background-position:0 -73px;
    }

#dream-home #pgallery #photonav .prev a.btn:active {
    background-position:0 -146px;
    }

#dream-home #pgallery #photonav .next a.btn:hover {
    background-position:right -292px;
    }

#dream-home #pgallery #photonav .next a.btn:active {
    background-position:right -365px;
    }


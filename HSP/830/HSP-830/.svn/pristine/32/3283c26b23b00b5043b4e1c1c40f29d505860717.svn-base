/** rounded corners markup is IE7 only; don't target 6 (it stays blocky) */
/* IE7 rounded corners white bg*/
.cnr-tl {
    display:block;
    height:2px;
    width:2px;
    position:absolute;
    top:0;
    left:0;
    background:transparent url(../imgs/ie/cnr-mask-wh-tl.png) no-repeat 0 0;
    }

.cnr-tr {
    display:block;
    height:2px;
    width:2px;
    position:absolute;
    top:0;
    right:0;
    background:transparent url(../imgs/ie/cnr-mask-wh-tr.png) no-repeat 0 0;
    }

.cnr-bl {
    display:block;
    height:2px;
    width:2px;
    position:absolute;
    bottom:0;
    left:0;
    background:transparent url(../imgs/ie/cnr-mask-wh-bl.png) no-repeat 0 0;
    }

.cnr-br {
    display:block;
    height:2px;
    width:2px;
    position:absolute;
    bottom:0;
    right:0;
    background:transparent url(../imgs/ie/cnr-mask-wh-br.png) no-repeat 0 0;
    }

/* 1px corner notches for IE7 == 2px border-radius*/
.notch-tl {
    display:block;
    height:1px;
    width:1px;
    position:absolute;
    top:0;
    left:0;
    background:#fff;
    }

.notch-tr {
    display:block;
    height:1px;
    width:1px;
    position:absolute;
    top:0;
    right:0;
    background:#fff;
    }

.notch-bl {
    display:block;
    height:1px;
    width:1px;
    position:absolute;
    bottom:0;
    left:0;
    background:#fff;
    }

.notch-br {
    display:block;
    height:1px;
    width:1px;
    position:absolute;
    bottom:0;
    right:0;
    background:#fff;
    }

/* IE7 rounded borders */
.bdr-tl {
    display:block;
    height:3px;
    width:3px;
    position:absolute;
    top:-1px;
    left:-1px;
    background:transparent url(../imgs/ie/bdr-tl.png) no-repeat 0 0;
    }

.bdr-tr {
    display:block;
    height:3px;
    width:3px;
    position:absolute;
    top:-1px;
    right:-1px;
    background:transparent url(../imgs/ie/bdr-tr.png) no-repeat 0 0;
    }

.bdr-bl {
    display:block;
    height:3px;
    width:3px;
    position:absolute;
    bottom:-1px;
    left:-1px;
    background:transparent url(../imgs/ie/bdr-bl.png) no-repeat 0 0;
    }

.bdr-br {
    display:block;
    height:3px;
    width:3px;
    position:absolute;
    bottom:-1px;
    right:-1px;
    background:transparent url(../imgs/ie/bdr-br.png) no-repeat 0 0;
    }

.bdr-tl.gold {
    background-image:url(../imgs/ie/bdr-gold-tl.png);
    }

.bdr-tr.gold {
    background-image:url(../imgs/ie/bdr-gold-tr.png);
    }

.bdr-bl.gold {
    background-image:url(../imgs/ie/bdr-gold-bl.png);
    }

.bdr-br.gold {
    background-image:url(../imgs/ie/bdr-gold-br.png);
    }


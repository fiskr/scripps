/* *** Color Picker swatch definitions *** */
ul.color-pick {
    margin:10px 0 4px 7px;
    }

#hg-ww .color-pick li {
    height:28px;
    width:27px;
    }

#hg-ww .color-pick li a {
    height:27px;
    width:27px;
    }

.color-pick li {
    display:block;
    float:left;
    height:43px;
    width:42px;
    margin-right:1px;
    margin-bottom:1px;
    }

.color-pick li a {
    display:block;
    height:42px;
    width:42px;
    text-indent:-999em;
    background-color:#efefef;
    border-bottom:1px solid rgba(0, 0, 0, 0.2);
    box-shadow:inset 0 0 0 1px rgba(255, 255, 255, 0.1), inset 0 0 38px 0 rgba(0, 0, 0, 0.1);
    background:-moz-linear-gradient(-45deg, rgba(255, 255, 255, 0.45) 0%, rgba(255, 255, 255, 0) 100%); /* FF3.6+ */
    background:-webkit-gradient(linear, left top, right bottom, color-stop(0%, rgba(255, 255, 255, 0.45)), color-stop(100%, rgba(255, 255, 255, 0))); /* Chrome,Safari4+ */
    background:-webkit-linear-gradient(-45deg, rgba(255, 255, 255, 0.45) 0%, rgba(255, 255, 255, 0) 100%); /* Chrome10+,Safari5.1+ */
    background:-o-linear-gradient(-45deg, rgba(255, 255, 255, 0.45) 0%, rgba(255, 255, 255, 0) 100%); /* Opera 11.10+ */
    background:-ms-linear-gradient(-45deg, rgba(255, 255, 255, 0.45) 0%, rgba(255, 255, 255, 0) 100%); /* IE10+ */
    background:linear-gradient(-45deg, rgba(255, 255, 255, 0.45) 0%, rgba(255, 255, 255, 0) 100%); /* W3C */
    filter:progid:dximagetransform.microsoft.gradient(startColorstr = '#80ffffff', endColorstr = '#00ffffff', GradientType = 1); /* IE6-9 fallback on horizontal gradient */
    outline:none;
    }

.color-pick li a:hover {
    opacity:0.9;
    }

.color-pick li a:active {
    box-shadow:inset 0 0 0 1px rgba(255, 255, 255, 0.14), inset 0 0 44px 0 rgba(0, 0, 0, 0.35);
    }

.color-pick li.brown a {
    background-color:#8c634a;
    }

.color-pick li.neutral a {
    background-color:#EAE5CF;
    }

.color-pick li.white a {
    background-color:#fff;
    border:1px solid #ddd;
    border:1px solid rgba(0, 0, 0, 0.14);
    border-bottom:1px solid rgba(0, 0, 0, 0.2);
    box-shadow:none;
    width:40px;
    height:41px;
    }

.color-pick li.white a:hover {
    background-color:#fcfcfc;
    opacity:1;
    border:1px solid #ccc;
    border:1px solid rgba(0, 0, 0, 0.21);
    border-bottom:1px solid rgba(0, 0, 0, 0.3);
    }

.color-pick li.white a:active {
    box-shadow:inset 0 0 0 1px rgba(255, 255, 255, 0.14), inset 0 0 44px 0 rgba(0, 0, 0, 0.05);
    }

.color-pick li.beige a {
    background-color:#EBE8DF;
    }

.color-pick li.blue a {
    background-color:#2E74B0;
    }

.color-pick li.green a {
    background-color:#7F9F5F;
    }

.color-pick li.gray a {
    background-color:#8C8C8C;
    }

.color-pick li.red a {
    background-color:#D43C37;
    }

.color-pick li.yellow a {
    background-color:#FAE257;
    }

.color-pick li.black a {
    background-color:#000;
    }

.color-pick li.orange a {
    background-color:#E88946;
    }

.color-pick li.gold a {
    background-color:#EBC82D;
    }

.color-pick li.pink a {
    background-color:#EB81AF;
    }

.color-pick li.purple a {
    background-color:#9C4899;
    }

.color-pick li.silver a {
    background-color:#ccc;
    }

.color-pick li.copper a {
    background-color:#CB804E;
    }

.color-pick li.turquoise a {
    background-color:turquoise;
    }

.color-pick li.mixed.color a {
    background:url(../imgs/multi-color-swatch.png) 0 0 no-repeat;
    }

#hg-ww .color-pick li.mixed.color a {
    background:url(../imgs/multi-color-swatch.png) -8px -7px no-repeat;
    }


/*site sub nav*/
.hg-site-subnav {
    background:#FFFFFF;
    margin:0 auto;
    width:994px;
    *z-index:-10;
    *position:relative;
    }

.hg-site-subnav ul {
    margin:5px 0 10px;
    margin-top:10 px\9;
    }

.hg-site-subnav ul li {
    margin-right:14px;
    padding:0;
    position:relative;
    }

.hg-site-subnav ul li a em {
    border-left:4px solid transparent;
    border-right:4px solid transparent;
    border-top:4px solid #CCCCCC;
    height:0;
    position:absolute;
    right:-13px;
    top:4px;
    width:0;
    padding-bottom:15px;
    /*IE Hacks*/
    top:15 px\9;
    *top:9px;
    }

.hg-site-subnav ul li.selected {
    margin-right:17px;
    }

.hg-site-subnav ul li.selected a em {
    right:12px;
    }

.hg-site-subnav ul li.selected.emroom a {
    padding-right:24px;
    color:#fff;
    }

.hg-site-subnav ul li a:hover, .hg-site-subnav ul > li:hover > a, .hg-site-subnav ul li.hover > a {
    color:#a8bfde;
    text-decoration:none;
    }

.hg-site-subnav ul li a:hover em, .hg-site-subnav ul > li:hover > a em, .hg-site-subnav ul li.hover > a em {
    border-top:4px solid #a0a0a0;
    }

.hg-site-subnav ul li.sel a:hover {
    color:#8f8f8f;
    }

.hg-site-subnav ul li.sel em {
    border-top:4px solid #a0a0a0;
    }

/*site sub nav dropdown*/
.hg-site-subnav .hg-site-subnav-dropdown {
    position:relative;
    display:inline;
    visibility:hidden;
    }

.hg-site-subnav li .hg-site-subnav-dropdown.visible {
    visibility:visible;
    }

.hg-site-subnav .hg-site-subnav-dropdown .arrow-up {
    position:absolute;
    top:10px;
    width:0;
    z-index:1001;
    background:url("../imgs/header-sprite.png") no-repeat scroll 0 -85px transparent;
    height:15px;
    width:24px; /*IE Hacks*/
    *top:25px;
    }

.hg-site-subnav li.selected .hg-site-subnav-dropdown .arrow-up {
    border-top:none;
    }

.hg-site-subnav li.selected .hg-site-subnav-dropdown ul li a {
    color:#386CB4;
    border-radius:0px;
    background:none;
    }

.hg-site-subnav li.selected .hg-site-subnav-dropdown ul li a:hover {
    color:#ffffff;
    background:#ef4141;
    }

.hg-site-subnav .hg-site-subnav-dropdown ul {
    background-color:#FFFFFF;
    box-shadow:3px 3px 4px rgba(127, 127, 127, 0.45);
    clear:both;
    display:block;
    left:-65px;
    padding:10px;
    position:absolute;
    top:18px;
    width:160px;
    z-index:1000;
    border:1px solid #e1e6e5;
    /*IE Hacks*/
    top:28 px\9;
    }

.hg-site-subnav .hg-site-subnav-dropdown ul li {
    margin:0;
    padding:0;
    width:160px;
    }

.hg-site-subnav .hg-site-subnav-dropdown ul.tenplus {
    width:320px;
    }

.hg-site-subnav .hg-site-subnav-dropdown ul.tenplus li {
    float:left;
    }

.hg-site-subnav .hg-site-subnav-dropdown ul li a {
    padding:6px 7px;
    display:block;
    min-height:14 px\9;
    width:140 px\9;
    }

.hg-site-subnav .hg-site-subnav-dropdown ul li a:hover {
    background:#ef4141;
    color:#ffffff;
    }


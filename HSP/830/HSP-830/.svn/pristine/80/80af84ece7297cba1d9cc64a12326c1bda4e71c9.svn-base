#hg-sitenav .subnav li.nav em.arrow-r {
    border-bottom:6px solid transparent;
    border-left:7px solid #ccc;
    border-top:6px solid transparent;
    float:right;
    height:0;
    margin:-26px 13px 0 0;
    width:0;
    cursor:pointer;
    }

#hg-sitenav .subnav li.nav.dd-on em.arrow-r {
    border-left-color:#a22c2c;
    }

/*  turn off "Target" tab on "Shop" menu (marketplace can turn back on */
#hg-sitenav li.tab-shop li.tab-target {
    display:none;
    }


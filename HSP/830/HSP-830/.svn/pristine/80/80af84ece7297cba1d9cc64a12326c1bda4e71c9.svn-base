/* Site Nav */
#hg-sitenav {
    height:48px;
    margin-top:7px;
    background:#525252;
    width:100%;
    position:relative;
    }

/* Site Nav - top tabs (super-sections) */
#hg-sitenav .topwrap {
    position:relative;
    width:994px;
    margin:0 auto;
    }

#hg-sitenav .topnav {
    *display:inline;
    clear:both;
    }

#hg-sitenav .topnav > li {
    position:relative;
    float:left;
    padding:14px;
    top:2px;
    }

#hg-sitenav .topnav > li.tab-home {
    padding-left:0;
    margin-left:-8px;
    margin-right:0px;
    top:0;
    }

#hg-sitenav .topnav > li.tab-home a {
    text-indent:-9999em;
    display:block;
    width:34px;
    height:31px;
    background:url('../imgs/header-sprite.png') no-repeat 9px 5px;
    margin:-5px -17px -7px 0;
    }

#hg-sitenav .topnav > li.tab-home a:hover {
    background:url('../imgs/header-sprite.png') no-repeat 9px -22px;
    }

#hg-sitenav .topnav > li.tab-home.savesel a {
    background:#383838 url('../imgs/header-sprite.png') no-repeat 9px 5px;
    border-radius:4px;
    -moz-border-radius:4px;
    margin-top:-6px;
    }

#hg-sitenav .topnav > li.tab-home.savesel a:hover {
    background:#383838 url('../imgs/header-sprite.png') no-repeat 9px -22px;
    }

#hg-sitenav .topnav > li.tab-home.savesel {
    background-color:#525252;
    margin-left:0;
    padding-right:21px !important;
    padding-left:0;
    padding-bottom:12px;
    }

#hg-sitenav .topnav li:hover a.toplink {
    text-decoration:none;
    color:#a9a9a9;
    }

#hg-sitenav .toplink {
    display:block;
    font:bold 16px arial;
    color:#FFF;
    cursor:pointer;
    }

#hg-sitenav .toplink em {
    position:absolute;
    top:21px;
    right:0;
    width:0;
    height:0;
    border-left:4px solid transparent;
    border-right:4px solid transparent;
    border-top:4px solid #313131;
    }

#hg-sitenav .sel .toplink {
    color:#fff;
    }

#hg-sitenav em.arrow-up {
    border-bottom:10px solid #F7F7F7;
    border-left:10px solid transparent;
    border-right:10px solid transparent;
    height:0;
    position:absolute;
    top:-9px;
    width:0;
    /*IE Hack*/
    border-bottom:10px solid white\9;
    }

#hg-sitenav em.arrow-up:after {
    border-bottom:9px solid white;
    border-left:9px solid transparent;
    border-right:9px solid transparent;
    content:"";
    height:0;
    left:-9px;
    position:absolute;
    top:1px;
    width:0;
    }

#hg-sitenav .topnav > li.savesel em.arrow-up {
    top:-9px;
    }

#hg-sitenav .topnav > li.tab-on-tv .subnav {
    left:-94px;
    }

#hg-sitenav .topnav > li.tab-blog .subnav {
    left:-30px;
    }

#hg-sitenav .topnav > li.tab-rooms .subnav .arrow-up {
    left:34px;
    }

#hg-sitenav .topnav > li.tab-decorating .subnav .arrow-up {
    left:50px;
    }

#hg-sitenav .topnav > li.tab-outdoors .subnav .arrow-up {
    left:47px;
    }

#hg-sitenav .topnav > li.tab-photos .subnav .arrow-up {
    left:35px;
    }

#hg-sitenav .topnav > li.tab-hgtv-dream-home .subnav .arrow-up {
    left:78px;
    }

#hg-sitenav .topnav > li.tab-blog .subnav .arrow-up {
    left:53px;
    }

#hg-sitenav .topnav > li.tab-on-tv .subnav .arrow-up {
    left:109px;
    }

#hg-sitenav .topnav > li.tab-shop .subnav .arrow-up {
    left:25px;
    }

/* selected tab */
#hg-sitenav .topnav > li.savesel {
    background-color:#fff;
    border-radius:4px 4px 0 0;
    padding:12px 28px 14px 14px;
    margin:4px 0 0 10px;
    min-height:20px;
    }

#hg-sitenav .topnav > li.savesel a {
    color:#333333;
    font-weight:bold;
    margin-top:-2px;
    }

#hg-sitenav .topnav > li.savesel a em {
    right:14px;
    top:17px;
    border-top:4px solid #ccc;
    }

#hg-sitenav .topnav > li.savesel:hover a em {
    border-top:4px solid #999;
    }

/* Site Nav - second row of tabs (sections) */

/** @fileoverview
 * 	styles for global header and footer
*/
.family-bar-wrap {
    background-color:#fefefe;
    width:100%;
    border-bottom:1px solid #e5e5e5;
    border-bottom:1px solid rgba(0, 0, 0, 0.08);
    }

.family-bar {
    height:35px;
    padding:0;
    margin:0 auto;
    min-width:994px;
    position:relative;
    width:994px;
    }

.family-bar ul {
    height:29px;
    padding-top:6px;
    float:left;
    }

.family-bar ul li {
    float:left;
    }

.family-bar ul li.current {
    background-color:#664991;
    border-radius:10px;
    }

.family-bar ul li.current:hover {
    -moz-opacity:.50;
    filter:alpha(opacity = 50);
    opacity:.50;
    }

.family-bar ul li a {
    padding:0 14px 0 14px;
    font-size:11px;
    font-weight:bold;
    color:#386cb4;
    }

.family-bar ul li.current a {
    color:#fff;
    }

.family-bar ul li.current a:hover {
    text-decoration:none;
    }

/* to do: autosuggest drops: */
#asug.flyout {
    width:324px !important;
    padding:0px;
    overflow:hidden;
    left:687px !important;
    top:138px !important; /* left: 0 !important; top: 1px !important; */
    }

#asug.flyout ul {
    *position:relative;
    list-style-position:outside;
    list-style:none;
    padding:0;
    margin:0;
    }

#asug.flyout ul.scroll {
    height:175px;
    *height:180px;
    overflow:auto;
    }

#asug.flyout iframe {
    display:none;
    position:absolute;
    top:0;
    left:0;
    z-index:-1;
    filter:mask();
    width:3000px;
    height:3000px;
    }

#asug.flyout li {
    color:#bfbfbf;
    margin:0;
    padding:3px 0 3px 16px;
    cursor:pointer;
    display:block;
    overflow:hidden;
    font-size:14px;
    }

#asug.flyout li em {
    font-style:normal;
    font-weight:normal;
    color:#363636;
    }

#asug.flyout li.ac_over {
    color:#ffffff;
    background:#EF4141 !important;
    left:-1px;
    position:relative;
    width:268px;
    }

#asug.flyout li.ac_over em {
    color:#ffffff;
    }

#asug.flyout .fly-bd h3 {
    font-size:11px;
    color:#999999;
    position:absolute;
    right:0;
    top:15px;
    font-weight:normal;
    padding-right:15px;
    }

#asug.flyout .fly-bd h3.on {
    color:#ffffff;
    }

#asug.flyout .fly-bd .close {
    background:url(../imgs/sp-popups.png) no-repeat;
    width:11px;
    height:11px;
    text-indent:-9999px;
    display:none;
    float:right;
    margin-top:3px;
    }

#asug.flyout .fly-hd {
    left:35px;
    position:absolute;
    top:30px;
    width:0;
    z-index:7000;
    background:url("../imgs/header-sprite.png") no-repeat scroll 0 -85px transparent;
    height:15px;
    width:30px;
    top:-2px;
    }

#asug.flyout .fly-bd {
    background-color:#FFFFFF;
    border:1px solid #E1E6E5;
    box-shadow:3px 3px 13px rgba(127, 127, 127, 0.45);
    clear:both;
    display:block;
    position:relative;
    top:11px;
    margin:0 20px 30px;
    padding:10px 0;
    z-index:6000;
    width:284px; /*IE Hacks*/
    left:0 px\9;
    }

/* *********************************************************** */

#leaderboard-wrap {
    background:#363636;
    border-bottom:1px solid #8e9794;
    margin-top:-1px;
    position:relative;
    z-index:3000;
    }

#leaderboard {
    margin:0 auto;
    width:728px; /* served in-line: padding: 8px 0 6px; */
    }

#leaderboard img {
    display:block;
    }

/* site header wrapper & container */
#hg-hd-outer {
    margin:0 auto;
    *position:relative;
    *z-index:1000;
    }

#hg-hd {
    background:#FFF;
    width:100%;
    min-width:994px;
    margin:0 auto;
    position:relative;
    border-bottom:1px solid #D1D6D5;
    }

/* logo */
#hd-logo {
    width:146px;
    height:75px;
    }

#hd-logo a {
    background:url("../imgs/header-sprite.png") no-repeat scroll -42px -113px white;
    display:block;
    height:80px;
    width:150px;
    }

#hd-logo a:hover {
    -moz-opacity:.50;
    filter:alpha(opacity = 50);
    opacity:.50;
    }

#hd-logo img {
    display:none;
    }

#hd-wrap {
    margin:0 auto;
    width:994px;
    height:75px;
    padding:14px 0 10px;
    position:relative;
    z-index:1001;
    }

/* subnav style */
.subnav-style {
    font:bold 11px arial;
    }

.subnav-style ul li {
    display:inline-block;
    *display: inline;
    }

.subnav-style.mobile ul li {float:left;}

.subnav-style ul li a {
    color:#386CB4;
    font:bold 11px arial;
    }

.subnav-style ul li a:hover {
    text-decoration:underline;
    }

.subnav-style ul li.selected a {
    color:#ffffff;
    background:#383838;
    padding:4px 14px 4px 14px;
    border-radius:4px;
    }

.subnav-style ul li.selected a:hover, .subnav-style ul li.selected:hover a, .subnav-style ul li.selected.hover > a {
    text-decoration:none;
    background:#606060;
    color:#fff;
    }

.subnav-style ul li.selected a.selem {
    padding-right:26px;
    }

.subnav-style ul li.emroom {
    margin-right:28px;
    padding-bottom:15px;
    }

/* dont miss */
#hd-dont-miss {
    left:170px;
    position:absolute;
    top:65px;
    }

#hd-dont-miss h4 {
    color:#EF4141;
    font-weight:bold;
    text-transform:uppercase;
    float:left;
    }

#hd-dont-miss ul {
    float:left;
    }

#hd-dont-miss ul li {
    margin-left:13px;
    float:left;
    }

#hd-dont-miss ul li.selected {
    margin-top:-4px;
    }

#hd-dont-miss ul li a {
    display:block;
    }

/* social icons */
#hd-social {
    height:65px;
    position:absolute;
    right:14px;
    top:8px; /*negative z-index for ie*/
    z-index:-1;
    }
#hd-social.mobile {right:0; top:58px; height:auto; right:-3px;}

#hd-social.mobile ul li {float:left; margin-right:8px;}
#hd-social ul li {
    margin:0 0 3px;
    display: inline-block;
    }

#hd-social ul li a {
    background:url("../imgs/header-sprite.png") no-repeat scroll 0 0 transparent;
    display:block;
    height:23px;
    width:23px;
    }

#hd-social ul li#hd-facebook a {
    background-position:-63px 0;
    }

#hd-social ul li#hd-twitter a {
    background-position:-97px 0px;
    }

#hd-social.mobile ul li#hd-pinterest {float:left; margin-right:8px;}
#hd-social ul li#hd-pinterest a {
    background-position:-131px 1px;
    }

#hd-social ul li#hd-google a {
    background-position:-165px 1px;
    /*display:none; put back in 09/26/13 */
    }

#hd-social ul li a:hover {
    -moz-opacity:.50;
    filter:alpha(opacity = 50);
    opacity:.50;
    }

/* header: site search */
/* footer search too */

/* overrides for header */
#hg-hd .search {
    position:absolute;
    top:92px;
    right:0;
    z-index:10;
    padding:1px;
    border-radius:4px;
    background-image:-webkit-gradient(linear, left bottom, left top, color-stop(rgb(0, 0, 0)), color-stop(rgb(184, 184, 184)));
    background-image:-moz-linear-gradient(center bottom, #525252, #B8B8B8);
    /*IE Hack*/
    z-index:-10 \9;
    }

#hg-hd #hgSearchForm.global-search {
    background:#FFF;
    width:193px;
    padding:3px 0 0 14px;
    border-radius:4px;
    -moz-box-shadow:inset 1px 2px 1px #EDEDED;
    -webkit-box-shadow:inset 1px 2px 1px #EDEDED;
    box-shadow:inset 1px 2px 1px #EDEDED;
    border:1px solid #d0d0d0;
    }

#hg-hd #hgSearchForm.global-search .search-in {
    float:left;
    margin:2px 4px 0 0;
    line-height:28px;
    color:#989898;
    font-family:arial, sans-serif;
    font-size:77%;
    }

#hg-hd #hgSearchForm.global-search .input {
    width:150px;
    float:left;
    }

#hg-hd #hgSearchForm.global-search .input input {
    border:0;
    font-size:14px;
    color:#999;
    width:150px;
    padding:16px 0 19px;
    }

#hg-hd #hgSearchForm.global-search .input input.ac-overlay-input {
    display:none;
    }

#hg-hd #hgSearchForm.global-search .input.focus input {
    width:100%;
    }

#hg-hd #hgSearchForm.global-search .input input.ac_loading {
    background:url('../imgs/auto-sug-loader.gif') no-repeat right center;
    position:relative;
    }

#hg-hd #hgSearchForm.global-search button {
    position:absolute;
    right:11px;
    width:28px;
    height:22px;
    background:url(../imgs/header-sprite.png) -28px -28px no-repeat;
    cursor:pointer;
    border:0;
    overflow:hidden;
    top:20px;
    }

#hg-hd #hgSearchForm.global-search button span {
    background:none;
    text-indent:-9999em;
    }

#hg-hd #hgSearchForm.global-search button:hover {
    background-position:-28px -2px;
    }

#hg-hd #hgSearchForm.global-search select {
    display:none;
    position:absolute;
    left:-999em;
    width:96px;
    margin-top:7px;
    margin-right:2px;
    font-size:85%;
    }


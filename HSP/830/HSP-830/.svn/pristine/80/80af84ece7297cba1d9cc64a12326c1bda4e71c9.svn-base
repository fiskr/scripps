    display:none;
    clear:both;
    box-shadow:3px -1px 13px rgba(127, 127, 127, 0.45);
    width:160px;
    z-index:7000;
    position:absolute;
    top:46px;
    left:0;
    padding:7px 0;
    background-color:#FFF;
    text-align:left;
    }

#hg-sitenav .sel:hover .subnav {
    display:block;
    }

#hg-sitenav .savesel .subnav {
    top:42px;
    }

#hg-sitenav .subnav li.nav {
    }

#hg-sitenav .subnav li.nav:last-child a {
    border-bottom:none;
    }

#hg-sitenav .subnav li.nav a {
    border-bottom:1px solid #F7F7F7;
    color:#386CB4;
    display:block;
    font:bold 12px arial;
    padding:12px 18px 12px 14px;

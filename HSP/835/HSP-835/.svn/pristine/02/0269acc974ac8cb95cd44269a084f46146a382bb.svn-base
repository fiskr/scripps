/* WEEKEND-LEAD: Dynamic Full-BD Lead */
.weekend-lead { margin: 0 14px 14px; height: 300px; position: relative;}
.weekend-lead .hd { width: 538px; background: #333; color: #fff; padding: 35px 14px; float: right; height: 230px;}
	.weekend-lead .hd h2 { font-size: 230%; line-height: 1; font-weight: bold; text-transform: uppercase; margin-bottom: 7px;}
	.weekend-lead .hd p { font-size: 108%; line-height: 1.28;}
.weekend-lead .lead-images { float: left;}
.weekend-lead .lead-menu { position: absolute; right: 7px; bottom: 7px; background: #fff; width: 616px; z-index: 300;}
	.weekend-lead .lead-menu ul { height: 185px;}
	.weekend-lead .lead-menu li { height: 157px; width: 205px; padding: 14px 0; }
	.weekend-lead .lead-menu li .bd { border-left: 1px solid #ddd; height: 100%; overflow: auto; padding: 0 14px;}
	.weekend-lead .lead-menu li.first .bd { border-left: none;}
.weekend-lead .dl-menu li { background: url(../imgs/se/weekend-lead.gif) no-repeat center top; }	
.weekend-lead .dl-menu li.active { background-image: url(../imgs/se/weekend-lead-active.gif); }
.weekend-lead .dl-content ul { position: relative; z-index: 200; }
.weekend-lead .dl-content li { position: absolute; width: 400px; height: 300px;}
.weekend-lead .dl-content li img { display: block; width: 400px; height: 300px;}
.weekend-lead h3.slug {color: #671104; font-weight: bold; line-height: 1; font-size: 85%; text-transform: uppercase; margin-bottom: 7px;}
.weekend-lead .bd .title { font-weight: bold; font-size: 161.6%; *font-size:153.9%; line-height: 1.05; margin-bottom: 4px;}
.weekend-lead .bd p { line-height: 1.38;}
.weekend-lead .bd p.more { font-size: 85%; line-height: 1.64;}

/* WEEKDAY-LEAD: Static Weekday leads. Needs a Major/Minor panel */
.weekday-lead { margin: 0 14px 14px; height: 300px;}
.weekday-lead h3.slug {color: #671104; font-weight: bold; line-height: 1; font-size: 85%; text-transform: uppercase; margin-bottom: 7px;}
.weekday-lead .panel { float: left;}
.weekday-lead .major { width: 602px; background: #333; color: #fff;}
.weekday-lead .major .panel { border: 7px solid #333; padding: 6px 7px 7px; height: 272px; width: 174px; overflow: auto; }
.weekday-lead .major h3.slug { margin-bottom: 12px;}
.weekday-lead .minor { border: 7px solid #333; width: 322px; background: #fff; padding: 7px 14px; height: 272px;}
	.weekday-lead .minor a.thumb { display: block; margin: 0 auto; width: 120px; margin-bottom: 14px; }
	.weekday-lead .minor a.thumb img {display:block;}
	.weekday-lead .minor .panel { margin-bottom: 7px; width: 161px; line-height: 1.38; height: 230px;}
	.weekday-lead .minor .panel .bd {height: 205px; overflow: auto; }
	.weekday-lead .minor .pane1 .bd { padding-right: 14px; }
	.weekday-lead .minor .pane2 h3 { padding-left: 14px;}
	.weekday-lead .minor .pane2 .bd { border-left: 1px solid #ddd; padding-left: 14px;}
	.weekday-lead .minor .more { clear: both; border-top: 1px solid #ddd; padding-top: 14px;}
	.weekday-lead .minor .more p { line-height: 1; *zoom:1;}
.weekday-lead .major h3.slug { color: #fff;}
.weekday-lead .major img { display: block; float: left;}
.weekday-lead .major a:link, .weekday-lead .major a:visited { color: #d8c5a5; }
.weekday-lead .major a:hover { color:#d8c5a5;}
.weekday-lead .major a:active { color: #fff;}

.guests-pod li { margin-bottom: 12px; }
#diy-we .guests-pod h4 { font-weight: bold; text-transform: uppercase; font-size: 108%; line-height: 1; margin-bottom: 14px; }

/* RESOURCES POD : exists in diy-we [no color] and diy-e [wood]*/
.resource-pod ul {line-height: 1.4;}
.resource-pod .first {border-top: 0; padding-top:0; margin-top: 0;}

#find-a-project .input-box { float: left; width: 196px; }
#find-a-project .date-box { width: 81px; }
#find-a-project .date-to-label { width: auto; margin: 0; padding: 0 6px 0 9px; }
#find-a-project .submit { margin-bottom: 2px; text-align: center; }

.network-pod .feature h5 { background: #d8c5a5; color: #391900;}
.network-pod .feature .bd { background: #f1efe4;}
.network-pod .ft { margin: 6px 6px 0; font-size: 85%; line-height: 1; color: #999;}
.network-pod .ft .show-links { float: left;}
.network-pod .ft .time-info { float: right;}

/* find project inside of network pod */

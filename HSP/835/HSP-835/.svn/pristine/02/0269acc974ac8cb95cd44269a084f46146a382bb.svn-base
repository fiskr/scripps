#how-to-videos .videos a.thumbnail { display: block; float: left; margin-right: 7px; line-height: 1; }
#how-to-videos .videos a.thumbnail img { border: 2px solid #241f19; }
#how-to-videos .videos .title { line-height: 1.38;}
#how-to-videos .videos .description { font-size: 85%; line-height: 1.46;}

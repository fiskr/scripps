#diy-e .sponsored-pod .image { float: right;}
#diy-e .sponsored-pod .ft { border-top: 1px solid #ddd; padding: 7px 0 0; margin: 7px 13px 0; font-size: 77%; color: #999; text-align: center; line-height: 2.82;}

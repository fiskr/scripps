.email-reminder-signup h3 { font-size: 18px; font-weight: bold; margin-top: -8px; font-size: 20px; }
.email-reminder-signup p { line-height: 20px; margin-bottom: 10px; }
.email-reminder-signup .form-wrap { position: relative; }

.email-reminder-signup .rounded-input {
	background:#fff;
	border: 1px solid #ddd;
	height: 29px;
	margin: 0;
	padding: 0;
	position: relative;
	background: #fff;
	line-height: normal;
	border-radius: 3px;
	-webkit-border-radius: 3px;
	-webkit-box-shadow: inset 0 2px 1px #f0f0f0;
	-moz-box-shadow: inset 0 2px 1px #f0f0f0;
	-o-box-shadow: inset 0 2px 1px #f0f0f0;
	box-shadow: inset 0 2px 1px #f0f0f0;
}
.email-reminder-signup .rounded-input.focus {
	-webkit-box-shadow: inset 0 2px 1px #f0f0f0, 0 0 5px #D1EEFC;
	-moz-box-shadow: inset 0 2px 1px #f0f0f0, 0 0 5px #D1EEFC;
	-o-box-shadow: inset 0 2px 1px #f0f0f0, 0 0 5px #D1EEFC;
	box-shadow: inset 0 2px 1px #f0f0f0, 0 0 5px #D1EEFC;
}
.email-reminder-signup .rounded-input.error {
	border-color: #E7A7A8;
	background-color: #FDDBD7;
	-webkit-box-shadow: inset 0 2px 1px #F4CDC6;
	-moz-box-shadow: inset 0 2px 1px #F4CDC6;
	-o-box-shadow: inset 0 2px 1px #F4CDC6;
	box-shadow: inset 0 2px 1px #F4CDC6;
}
.email-reminder-signup .rounded-input .input { height: 34px; margin: 0 76px 0 0; padding: 0 12px; line-height: normal; }

.email-reminder-signup input[type=text] {
	width: 100%;
	height: 30px;
	line-height: 30px;
	padding: 0; 
	background: none; 
	border: none; 
	color: #aaa; 
	font-size: 14px;
}
.email-reminder-signup input[type=text]:focus,
.email-reminder-signup .error input[type=text] { color: #222; }

.email-reminder-signup button { position: absolute; right: -1px; top: -1px; margin: 0; }
.email-reminder-signup button em { font-weight: bold !important; }

.email-reminder-signup .reminder-loading {
	display: none;
	width: 100%;
	height: 100%;
	position: absolute;
	z-index: 1; 
	left: -1px; 
	top: -1px;
	text-align: center;
	background-color: #fff;
	border: 1px solid #f6f6f6;
	opacity: 0.8;
	filter: alpha(opacity=80);
}
.email-reminder-signup .reminder-loading p {
	position: relative;
	top: 0;
	margin: 0;
	font-weight: bold;
	margin-top: -8px;
}
.email-reminder-signup .reminder-loading p img {
	position: relative;
	top: 8px;
	padding: 4px;
	-webkit-border-radius: 2px;
	-moz-border-radius: 2px;
	-o-border-radius: 2px;
	border-radius: 2px;
	border: 1px solid #ccc;
	background: #fff;
}
.email-reminder-signup #valid-msg { margin: 0; padding: 0; }
.email-reminder-signup #valid-msg.pass { margin-bottom: 24px; }
.email-reminder-signup #valid-msg.fail { display: inline; color: #cc2414; font-style: italic; }



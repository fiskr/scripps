#diy-e .resource-pod li { margin-bottom: 12px; }





/* PROJECT-POD: use with .widget */
.project-pod .list li { line-height: 1.38; /*background-position: left 7px;*/}
.project-pod .steps li { line-height: 1; background: #f1efe4; border-bottom: 1px solid #fff; margin: 0 6px; padding-left: 24px; }
.project-pod .steps li.current { margin: 0 6px 0 0; background: #2f2a25; padding-left: 30px;}
.project-pod .steps a { display: block; padding: 7px; border-left: 1px solid #fff;}
.project-pod .steps li.current a { padding: 14px 7px; background: #fff; color: #333; font-weight: bold;}
.project-pod .current-project .hd { margin: 0 6px; background: #d8c5a5; padding: 7px 14px; border-bottom: 1px solid #fff;}
.project-pod .current-project .hd p { line-height: 1;}

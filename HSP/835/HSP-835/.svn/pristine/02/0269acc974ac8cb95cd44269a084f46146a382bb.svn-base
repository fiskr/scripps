.project-pod .related-projects .bd { background: #eee; padding: 13px 7px; border-bottom: 1px solid #fff;}
.project-pod .related-projects .bd { background: #eee; padding: 13px 7px; border-bottom: 1px solid #fff;}
#diy-e .project-pod .working-on li { background: none;border-bottom: 1px dotted #e1e1e1;line-height: 2;padding-left: 0;}
#diy-e .project-pod .working-on li.current {background: url(../imgs/se/sp-diy-icons.gif) no-repeat left -364px; padding-left: 14px;}

/* NETWORK-POD: kind of like a project-pod
	use with .widget */
.network-pod h5 { font-size: 123.1%; line-height: 1; font-weight: bold; text-transform: uppercase; padding: 7px 14px; background: #ccc; }
.network-pod .feature .hd, .network-pod .secondary .hd { margin: 0 6px;}
.network-pod .feature .bd, .network-pod .secondary .bd { margin: 0 6px; background: #eee; padding: 13px 7px; border-bottom: 1px solid #fff;}

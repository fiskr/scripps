/* jCarouselLite CSS */

/* generic carousel styles */
.crsl {
    position:relative;
    }

.crsl.pro-pod {
    padding:14px 6px;
    }

.crsl .prev-btn {
    display:block;
    background:transparent url(../imgs/hgtv_ui_207.png) no-repeat -267px -481px;
    height:18px;
    width:18px;
    text-indent:-9999px;
    z-index:100;
    cursor:pointer;
    }

.crsl .next-btn {
    display:block;
    background:transparent url(../imgs/hgtv_ui_207.png) no-repeat -285px -481px;
    height:18px;
    width:18px;
    text-indent:-9999px;
    z-index:100;
    cursor:pointer;
    }

.crsl .prev-btn:hover {
    background-position:-303px -481px;
    cursor:pointer;
    }

.crsl .next-btn:hover {
    background-position:-321px -481px;
    cursor:pointer;
    }

.crsl .prev-btn.active,
.crsl .prev-btn:active,
.crsl .prev-btn.selected {
    background-position:-339px -481px;
    cursor:default;
    }

.crsl .next-btn.active,
.crsl .next-btn:active,
.crsl .next-btn.selected {
    background-position:-357px -481px;
    cursor:default;
    }

.crsl .prev-btn.disabled {
    background-position:-375px -481px;
    cursor:default;
    }

.crsl .next-btn.disabled {
    background-position:-393px -481px;
    cursor:default;
    }

* html .crsl .prev-btn, * html .crsl .next-btn {
    line-height:18px;
    }

.crsl .nav {
    position:relative;
    margin:0 auto;
    }

.crsl .controls {
    position:absolute;
    }

.crsl .crsl-wrap {
    margin:10px 0 0;
    }

.crsl .crsl-wrap img {
    display:block;
    }

.crsl .crsl-wrap ul .crsl-item {

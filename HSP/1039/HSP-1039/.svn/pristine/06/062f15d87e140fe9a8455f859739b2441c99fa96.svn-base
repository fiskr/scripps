/*royalSlider CSS*/
/* v1.0.3 */
/* customized for Lead Carousel SE-7685 */
body#home.section h1 {
  display:none; /* hide Home text on home page; putting it here since change of page template type was motivated by lead carousel*/
}

.royalSlider {
    width: 600px; /* was 600px */
    /*height: 386px; was 400px */
    position: relative;
    direction: ltr;
    
}

.royalSlider > * {
    float: left;
}

#lead-carousel img   {
    border: none;
    margin-top: 0;
    padding: 0;
    width: 517px !important;
    height: auto !important;
    /*height: 388px !important;*/
}

#lead-carousel {
    margin: 13px;
    height: 386px;
    display: none;
    overflow: hidden;
    /*box-shadow:  -28px 0 28px -28px #999 inset;*/
}

#lead-carousel .panel {
    padding: 5px 0 9px 14px;
    position: absolute;
    width: 100%;
    bottom: 0;
    height: 61px;
    /*color: #386CB4;   makes non-links appear as links */
    /* Fallback for web browsers that doesn't support RGBa */
    background: rgb(51, 51, 51) transparent;
    /* RGBa with 0.8 opacity */
    background: rgba(51, 51, 51, 0.8);
    /* For IE 8*/
    background: black\9;
    filter:alpha(opacity=80); /*this makes the text opaque too, but it looks ok*/
    zoom: 1;
    color: white;
}

#lead-carousel .rsSlide:not(.rsActiveSlide) .panel{
     display: none;
}


#lead-carousel #hatch-left {
    background: url("../imgs/hgtv-homepage-carousel-pattern.png") repeat left top transparent;    
    height:100%;
    position: absolute;
    width: 38px;
    left:0;
    pointer-events: none;
}

#lead-carousel #hatch-right {
    background: url("../imgs/hgtv-homepage-carousel-pattern.png") repeat left top transparent;    
    height:100%;
    position: absolute;
    width: 38px;
    right:0;
    pointer-events: none;
}

/*.rsSlide:not(.rsActiveSlide){
    opacity: .50; 
}
*/

#lead-carousel .panel h4 a{
    color: white;
}

#lead-carousel .panel h4 a:hover{
    text-decoration: underline;
}
#lead-carousel h4 {
    font-size: 21px;
    font-weight: bold;
    line-height: 28px;

}

#lead-carousel .subhead{
    color: #fff;
    font-size: 14px;
    line-height: 18px;
    width: 490px;
}

#lead-carousel .subhead a {
    color: #73d1c8;
    padding-left: 9px;
}

#lead-carousel p a:hover {
    /*color: #fff;*/
    /*text-decoration: none;*/
}


#lead-carousel span.secondary {
    color: #fff;
    padding-left: 9px;
    /*margin: 0 10px;*/
}


/*#lead-carousel p a:nth-child(2){
    background:  url("../imgs/bullet-white.png") no-repeat scroll left 7px transparent;
    padding: 0 10px;
}*/


#lead-carousel p a:nth-child(2):before:hover {
    text-decoration: none;
}

#lead-carousel ul.two-col {
    float: left;
    width: 177px;
    padding-right: 7px;
}

#lead-carousel ul.two-col li {
    line-height: 21px;
}


.rsWebkit3d .rsSlide,
.rsWebkit3d .rsContainer,
.rsWebkit3d .rsThumbs,
.rsWebkit3d .rsPreloader,
.rsWebkit3d img,
.rsWebkit3d .rsOverflow,
.rsWebkit3d .rsBtnCenterer,
.rsWebkit3d .rsAbsoluteEl {
    -webkit-backface-visibility: hidden;
    -webkit-transform: translateZ(0); 
}
.rsFade.rsWebkit3d .rsSlide,
.rsFade.rsWebkit3d img,
.rsFade.rsWebkit3d .rsContainer {
    -webkit-transform: none;
}
.rsOverflow {
    width: 600px;
    height: 100%;
    position: relative;
    overflow: hidden;
    float: left;
    -webkit-tap-highlight-color:rgba(0,0,0,0);

}

.rsVisibleNearbyWrap{ 
    /*box-shadow:  28px 0 28px -28px #999 inset;*/
    -webkit-transform: translate3d(0, 0, 0); /*DRS: seems to remove odd page motions in webkit browsers*/
}


.rsSlide {
    opacity: .15;
    filter: alpha(opacity=15);
    -ms-filter:"progid:DXImageTransform.Microsoft.Alpha(Opacity=15)";
}


.rsVisibleNearbyWrap {
    width: 600px; /*was 100%*/
    height: 100%;
    position: relative;
    overflow: hidden;
    left: 0;
    top: 0;
    -webkit-tap-highlight-color:rgba(0,0,0,0);
}
.rsVisibleNearbyWrap .rsOverflow {
    position: absolute;
    left: 0;
    top: 0;

}
.rsContainer {
    position: relative;
    width: 100%;
    height: 100%;
    -webkit-tap-highlight-color:rgba(0,0,0,0);
}

.rsArrow,
.rsThumbsArrow {
    cursor: pointer;

}

.rsThumb {
    float: left;
    position: relative;
}


.rsArrow,
.rsNav,
.rsThumbsArrow {
    opacity: 1;
    -webkit-transition:opacity 0.3s linear;
    -moz-transition:opacity 0.3s linear;
    -o-transition:opacity 0.3s linear;
    transition:opacity 0.3s linear;
}

.rsHidden {
    opacity: 0;
    visibility: hidden;
    -webkit-transition:visibility 0s linear 0.3s,opacity 0.3s linear;
    -moz-transition:visibility 0s linear 0.3s,opacity 0.3s linear;
    -o-transition:visibility 0s linear 0.3s,opacity 0.3s linear;
    transition:visibility 0s linear 0.3s,opacity 0.3s linear;
}


.rsGCaption {
    width: 100%;
    float: left;
    text-align: center;
}

/* Fullscreen options, very important ^^ */
.royalSlider.rsFullscreen {
    position: fixed !important;
    height: auto !important;
    width: auto !important;
    margin: 0 !important;
    padding: 0 !important;
    z-index: 2147483647 !important;
    top: 0 !important;
    left: 0 !important;
    bottom: 0 !important;
    right: 0 !important;
}


.royalSlider .rsSlide.rsFakePreloader {
    opacity: 1 !important;
    -webkit-transition: 0s;
    -moz-transition: 0s;
    -o-transition:  0s;
    transition:  0s;
    display: none;
}


.rsSlide {
    position: absolute;
    left: 0;
    top: 0;
    display: block;
    overflow: hidden;
    
    height: 100%;
    width: 100%;
}

.royalSlider.rsAutoHeight,
.rsAutoHeight .rsSlide {
    height: auto;
}

.rsContent {
    width: 100%;
    height: 100%;
    position: relative;
}

.rsPreloader {
    position:absolute;
    z-index: 0; 
}

.rsNav {
    -moz-user-select: -moz-none;
    -webkit-user-select: none;
    user-select: none;
}
.rsNavItem {
    -webkit-tap-highlight-color:rgba(0,0,0,0.25);
}

.rsThumbs {
    cursor: pointer;
    position: relative;
    overflow: hidden;
    float: left;
    z-index: 22;
}
.rsTabs {
    float: left;
    background: none !important;
}
.rsTabs,
.rsThumbs {
    -webkit-tap-highlight-color:rgba(0,0,0,0);
    -webkit-tap-highlight-color:rgba(0,0,0,0);
}


.rsVideoContainer {
    /*left: 0;
    top: 0;
    position: absolute;*/
    /*width: 100%;
    height: 100%;
    position: absolute;
    left: 0;
    top: 0;
    float: left;*/
    width: auto;
    height: auto;
    line-height: 0;
    position: relative;
}
.rsVideoFrameHolder {
    position: absolute;
    left: 0;
    top: 0;
    background: #141414;
    opacity: 0;
    -webkit-transition: .3s;
}
.rsVideoFrameHolder.rsVideoActive {
    opacity: 1;
}
.rsVideoContainer iframe,
.rsVideoContainer video,
.rsVideoContainer embed,
.rsVideoContainer .rsVideoObj {
    position: absolute;
    z-index: 50;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
}
/* ios controls over video bug, shifting video */
.rsVideoContainer.rsIOSVideo iframe,
.rsVideoContainer.rsIOSVideo video,
.rsVideoContainer.rsIOSVideo embed {
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
    padding-right: 44px;
}

.rsABlock {
    left: 0;
    top: 0;
    position: absolute;
    z-index: 15;
    -webkit-backface-visibility: hidden;
}

.grab-cursor {
    cursor:url("../imgs/grab.png") 8 8, move; 
}

.grabbing-cursor{ 
    cursor:url("../imgs/grabbing.png") 8 8, move;
}

.rsNoDrag {
    cursor: auto;
}

.rsLink {
    left:0;
    top:0;
    position:absolute;
    width:100%;
    height:100%;
    display:block;  
    z-index: 20;
    background: url("../imgs/blank.gif");
    -webkit-backface-visibility: hidden;
}

/***** from skin ****/

.rsDefault .rsArrow {
    height: 100%;
    width: 44px;
    position: absolute;
    display: block;
    cursor: pointer;
    z-index: 21;
}
.rsDefault.rsVer .rsArrow {
    width: 100%;
    height: 44px;
    
}
.rsDefault.rsVer .rsArrowLeft { top: 0; left: 0; }
.rsDefault.rsVer .rsArrowRight { bottom: 0;  left: 0; }

.rsDefault.rsHor .rsArrowLeft { left: 0; top: 14px; }
.rsDefault.rsHor .rsArrowRight { right: 0; top:14px; }

.rsDefault .rsArrowIcn {        
    width: 34px; /* was 41*/
    height: 40px; /*was 80*/
    top: 160px;
    margin-top: -16px;  
    margin-left: 0px;
    position: absolute; 
    cursor: pointer;    
    background: url('../imgs/hgtv-homepage-carousel-sprite-100.png');
    background-color: transparent;
    *background-color: transparent;
    border-radius: 2px;
}

/* -85 --> -60, -45 --> -67 */
.rsDefault.rsHor .rsArrowLeft .rsArrowIcn { background-position: -0px -0px; left: 0;}
.rsDefault.rsHor .rsArrowLeft:hover .rsArrowIcn { background-position: -0px -60px;}
.rsDefault.rsHor .rsArrowRight .rsArrowIcn { background-position: -67px -0px; right: 0;}
.rsDefault.rsHor:hover .rsArrowRight:hover .rsArrowIcn { background-position: -67px -60px;}

.rsDefault.rsVer .rsArrowLeft .rsArrowIcn { background-position: -96px -32px; }
.rsDefault.rsVer .rsArrowRight .rsArrowIcn { background-position: -96px -64px; }

.rsDefault .rsArrowDisabled .rsArrowIcn { opacity: .2; filter: alpha(opacity=20);  *display: none; }


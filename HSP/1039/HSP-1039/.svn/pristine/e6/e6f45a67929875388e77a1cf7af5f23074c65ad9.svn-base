.wide-lead .super-lead {
    background:#fff;
    height:300px;
    width:968px;
    padding-top:14px;
    }

.triple-promo {
    height:300px;
    background:#f5f4f0;
    }

.triple-promo a {
    color:#66BFE7;
    }

.triple-promo a:hover {
    color:#8be0ff;
    }

.triple-promo p {
    color:#444;
    margin-bottom:7px;
    font-size:14px;
    }

.triple-promo p a {
    font-weight:normal;
    }

.triple-promo p.small {
    margin-bottom:0;
    }

.triple-promo h2 {
    color:#FFF;
    }

.triple-promo h4 {
    font-weight:bold;
    margin-bottom:7px;
    font-size:21px;
    color:#386cb4;
    }

.triple-promo h5 {
    margin-bottom:7px;
    color:#999;
    font-weight:bold;
    }

.triple-promo .panel {
    float:left;
    }

.triple-promo .major {
    width:630px;
    }

.triple-promo .major .panel {
    padding:20px;
    width:190px;
    overflow:auto;
    }

.triple-promo .minor {
    width:300px;
    height:258px;
    background:#fff;
    padding:14px;
    margin:7px 0;
    box-shadow:1px 1px 2px 2px rgba(0, 0, 0, 0.14);
    }

.triple-promo .minor a.thumb {
    display:block;
    margin:0 auto 7px;
    width:120px;
    }

.triple-promo .minor a.thumb img {
    display:block;
    }

.triple-promo .minor .panel {
    margin-bottom:7px;
    width:150px;
    }

.triple-promo .minor .panel .bd {
    height:221px;
    overflow:hidden;
    padding-right:7px;
    color:#386cb4;
    font-size:14px;
    }

.triple-promo .minor .panel .bd img {
    margin-bottom:10px;
    }

.triple-promo .minor .pane2 h3 {
    padding-left:14px;
    }

.triple-promo .minor .pane2 .bd {
    border-left:1px dotted #777;
    padding-left:14px;
    }

.triple-promo .minor .more {
    clear:both;
    border-top:1px dotted #777;
    padding-top:14px;
    font-size:14px;
    }

.triple-promo .minor .more p {
    font-weight:bold;
    line-height:1;
    *zoom:1;
    font-weight:bold;
    color:#999;
    }

.triple-promo .minor .more a {
    font-weight:bold;
    line-height:1;
    *zoom:1;
    font-weight:normal;
    color:#386cb4;
    }

.triple-promo .major img {
    display:block;
    float:left;
    }

#bd-wrapper .super-lead .major.panel img {
    background-color:none;
    border:none;
    padding:0;
    }

.triple-promo .major dl.proj-details {
    font-size:.856em;
    margin-top:20px;
    line-height:1.27em;
    }

.triple-promo .major dl.proj-details dt {
    font-weight:bold;
    float:left;
    margin-right:3px;
    }

.triple-promo .major p.more a {
    color:#386cb4;
    font-size:11px;
    }

.triple-promo .major .tune-in {
    font-size:.856em;
    margin-top:20px;
    line-height:1.27em;
    }

/* not to be confused with .super-lead */
.wide-lead #hg-superlead {
    overflow:hidden;
    width:994px;
    border-bottom:1px solid #dae7e3;
    }

.wide-lead #hg-superlead .four-cross, #hg-superlead .three-cross {
    overflow:hidden;
    }

.wide-lead #hg-superlead .pod {
    float:left;
    padding:8px 14px 14px 14px;
    border-left:1px solid #dae7e3;
    border-right:1px solid #dae7e3;
    margin-left:-1px;
    }

.wide-lead #hg-superlead .pod h2 {
    margin-bottom:4px;
    font-family:arial, sans-serif;
    font-size:138.5%;
    font-weight:bold;
    }

.wide-lead #hg-superlead .pod .thumb {
    display:block;
    margin-bottom:7px;
    }

.wide-lead #hg-superlead .pod h3 {
    font-family:'trebuchet ms', sans-serif;
    clear:both;
    margin-bottom:1px;
    font-weight:normal;
    font-size:92.3%;
    }

.wide-lead #hg-superlead .pod .cta {
    clear:both;
    display:block;
    margin-bottom:14px;
    }

.wide-lead #hg-superlead .pod .main-cta {
    font-weight:bold;
    font-size:100%;
    display:block;
    line-height:16px;
    margin:3px 0;
    }

.wide-lead #hg-superlead .pod p {
    line-height:18px;
    }

.wide-lead #hg-superlead .pod ul.list {
    margin-top:7px;
    }

.wide-lead #hg-superlead .pod .sub-item h3 {
    margin-top:14px;
    clear:both;
    }

.wide-lead #hg-superlead .pod .flat-list {
    overflow:hidden;
    margin-top:3px;
    }

.wide-lead #hg-superlead .pod .flat-list li {
    float:left;
    background:transparent url(../imgs/bullet-red.png) no-repeat scroll left 7px;
    padding-left:12px;
    margin-left:7px;
    line-height:18px;
    _float:none;
    _display:inline;
    }

.wide-lead #hg-superlead .pod .flat-list li.first {
    background:none;
    margin-left:0;
    padding-left:0;
    }

.wide-lead #hg-superlead .four-cross .t-1 {
    width:214px;
    border-left:none;
    margin-left:0;
    }

.wide-lead #hg-superlead .four-cross .t-2 {
    width:265px;
    background-color:#F1F6F2;
    }

.wide-lead #hg-superlead .four-cross .t-2 .main-cta {
    margin-top:-2px;
    }

.wide-lead #hg-superlead .four-cross .t-3 {
    width:274px;
    }

.wide-lead #hg-superlead .four-cross .t-4 {
    width:124px;
    border-right:none;
    }

.wide-lead #hg-superlead .three-cross .t-1 {
    width:126px;
    border-left:none;
    margin-left:0;
    }

.wide-lead #hg-superlead .three-cross .t-2 {
    width:482px;
    background-color:#f1f6f2;
    }

.wide-lead #hg-superlead .three-cross .t-2 p {
    margin-top:4px;
    }

.wide-lead #hg-superlead .four-cross .t-2 img {
    margin-bottom:7px;
    }

.wide-lead #hg-superlead .four-cross .t-2 object {
    *margin-bottom:7px;
    }

.wide-lead #hg-superlead .four-cross {
    background:transparent url('http://hgtv.sndimg.com/webhgtv/hg20/imgs/ie/four-cross-faux-bg.png') 0 0 repeat-y;
    padding-top:7px;
    }

.wide-lead #hg-superlead .three-cross .t-3 {
    width:300px;
    border-right:none;
    }

.wide-lead #hg-superlead .three-cross .t-3 .thumb {
    float:right;
    margin-left:7px;
    margin-bottom:7px;
    }

.wide-lead #hg-superlead .three-cross .sub-item {
    border-top:1px solid #dae7e3;
    margin-top:14px;
    }

/* reverse layout superlead for three-cross */
.wide-lead #hg-superlead.reverse .three-cross .pod.t-1 {
    border-left:1px solid #dae7e3;
    }

.wide-lead #hg-superlead.reverse .three-cross .pod.t-3 {
    border:none;
    }

.themed-lead {
    height:300px;
    position:relative;
    }

.themed-lead .hd {
    background:#f5f4f0;
    padding:14px 21px 21px;
    float:right;
    height:265px;
    width:526px;
    color:#fff;
    }

.themed-lead h2 {
    color:#555;
    font-size:35px;
    font-weight:bold;
    }

.themed-lead p {
    font-size:14px;
    color:#444;
    }

.themed-lead h5 {
    font-size:11px;
    font-weight:bold;
    color:#666;
    }

.themed-lead p.small a {
    font-size:11px;
    color:#386cb4;
    }

.themed-lead h4 a {
    font-size:21px;
    color:#386cb4;
    font-weight:bold;
    }

.themed-lead ul.menu li * {
    opacity:0.74;
    filter:alpha(opacity = 74);
    background:#fff;
    }

.themed-lead ul.menu li.active * {
    opacity:1;
    filter:alpha(opacity = 100);
    background:#fff;
    }

.themed-lead .lead-images {
    float:left;
    }

#bd-wrapper .themed-lead .lead-images ul li img {
    border:0;
    padding:0;
    }

.themed-lead .lead-menu {
    position:absolute;
    right:7px;
    bottom:7px;
    background:#FFF;
    width:616px;
    z-index:300;
    box-shadow:1px 1px 2px 2px rgba(0, 0, 0, 0.14);
    }

.themed-lead .lead-menu ul {
    height:190px;
    }

.themed-lead .lead-menu li {
    height:190px;
    width:205px;
    padding:0;
    margin:0;
    float:left;
    position:relative;
    }

.themed-lead .lead-menu li .bd {
    border-left:1px dotted #ddd;
    height:auto;
    overflow:hidden;
    height:155px;
    padding:14px 14px 0;
    }

.themed-lead .lead-menu li .bd p {
    line-height:21px;
    }

.themed-lead .lead-menu li.first .bd {
    border-left:none;
    }

.themed-lead .dl-menu li em {
    width:0;
    height:0;
    display:block;
    margin-left:100px;
    border-left:10px solid transparent;
    border-right:10px solid transparent;
    border-top:10px solid #dfdfdf;
    }

html body .themed-lead .dl-menu li.first p em, html body .themed-lead .dl-menu li.first.active p em,
html body .themed-lead .dl-menu li.second p em, html body .themed-lead .dl-menu li.second.active p em,
html body .themed-lead .dl-menu li.third p em, html body .themed-lead .dl-menu li.third.active p em {
    border:none;
    padding:0;
    margin:auto;
    height:auto;
    width:auto;
    display:inline;
    }

.themed-lead .dl-menu li.active em {
    border-top:10px solid #ef4141;
    }

.themed-lead .dl-content ul {
    position:relative;
    z-index:200;
    }

.themed-lead .dl-content li {
    position:absolute;
    width:400px;
    height:300px;
    }

.themed-lead .dl-content li img {
    display:block;
    width:400px;
    height:300px;
    }

